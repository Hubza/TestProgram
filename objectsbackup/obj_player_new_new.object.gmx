        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Free FMODGMS
{
    FMODGMS_Chan_StopChannel(channel);
    
    for (var i=0; i&lt;8; i++)
    {
        FMODGMS_Snd_Unload(sound[i]);
